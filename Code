password = input()

lenght = len(password)
upper= 0
lower =0
digit = 0

for char in password:
    if char.isupper():
        upper+=1
    elif char.islower():
        lower +=1
    elif char.isdigit():
        digit +=1

if (lenght >=8 and lenght <=12) and lower >=3 and upper>=2 and digit>=1:        
    print('Valid')
else:
    print('Invalid')    
